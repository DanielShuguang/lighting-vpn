{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "vpn",
  "version": "0.1.0",
  "identifier": "com.ohushuguang.vpn",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:14200",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "VPN 客户端",
        "width": 1000,
        "height": 700,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}